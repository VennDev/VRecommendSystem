        <!-- API Server -->
        <mxCell id="api-server" value="API Server (FastAPI + Python)" style="swimlane;fontStyle=1;align=center;verticalAlign=top;childLayout=stackLayout;horizontal=1;startSize=26;horizontalStack=0;resizeParent=1;resizeParentMax=0;resizeLast=0;collapsible=1;marginBottom=0;fillColor=#fff2cc;strokeColor=#d6b656;" vertex="1" parent="1">
          <mxGeometry x="600" y="100" width="450" height="380" as="geometry"/>
        </mxCell>
        <mxCell id="api-server-content" value="Port: 2030&#xa;&#xa;Core Responsibilities:&#xa;• User Authentication &amp; Authorization&#xa;• Session Management&#xa;• OAuth 2.0 Integration&#xa;• User Management&#xa;• Activity Logging&#xa;• Whitelist Management&#xa;• API Gateway to AI Server&#xa;• Rate Limiting&#xa;• Security &amp; Access Control&#xa;&#xa;Key Endpoints:&#xa;• /auth/* - Authentication routes&#xa;• /users/* - User management&#xa;• /admin/* - Admin operations&#xa;• /logs/* - Activity &amp; server logs&#xa;• /whitelist/* - Whitelist CRUD&#xa;• /ai/* - Proxy to AI Server&#xa;&#xa;Database:&#xa;• PostgreSQL (Primary)&#xa;• Tables: users, sessions, activity_logs,&#xa;  whitelisted_emails, server_logs&#xa;&#xa;Technology Stack:&#xa;• FastAPI&#xa;• SQLAlchemy ORM&#xa;• Pydantic&#xa;• OAuth2 with JWT&#xa;• Uvicorn ASGI Server" style="text;strokeColor=none;fillColor=none;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=0;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;" vertex="1" parent="api-server">
          <mxGeometry y="26" width="450" height="354" as="geometry"/>
        </mxCell>

        <!-- AI Server -->
        <mxCell id="ai-server" value="AI/ML Server (FastAPI + Python)" style="swimlane;fontStyle=1;align=center;verticalAlign=top;childLayout=stackLayout;horizontal=1;startSize=26;horizontalStack=0;resizeParent=1;resizeParentMax=0;resizeLast=0;collapsible=1;marginBottom=0;fillColor=#e1d5e7;strokeColor=#9673a6;" vertex="1" parent="1">
          <mxGeometry x="1150" y="100" width="500" height="480" as="geometry"/>
        </mxCell>
        <mxCell id="ai-server-content" value="Port: 9999&#xa;&#xa;Core Responsibilities:&#xa;• Machine Learning Model Training&#xa;• Recommendation Generation&#xa;• Model Management &amp; Registry&#xa;• Data Processing (DataChef)&#xa;• Background Task Scheduling&#xa;• Model Evaluation&#xa;&#xa;Key Components:&#xa;&#xa;1. Model Service&#xa;   • Train, load, save ML models&#xa;   • Support multiple algorithms (NMF, SVD, ALS, etc.)&#xa;   • Batch predictions&#xa;   • Model versioning&#xa;&#xa;2. DataChef Service&#xa;   • ETL from various sources (SQL, NoSQL, CSV, API)&#xa;   • Data transformation&#xa;   • Feature engineering&#xa;&#xa;3. Scheduler Service&#xa;   • Background task execution&#xa;   • Cron-based scheduling&#xa;   • Task monitoring&#xa;&#xa;4. Database Service&#xa;   • Multi-database support&#xa;   • Connection pooling&#xa;&#xa;Supported Algorithms:&#xa;• NMF (Non-negative Matrix Factorization)&#xa;• SVD (Singular Value Decomposition)&#xa;• ALS (Alternating Least Squares)&#xa;• KNN (K-Nearest Neighbors)&#xa;• Deep Learning models&#xa;&#xa;Database:&#xa;• MongoDB (Task execution history)&#xa;• PostgreSQL (Training data, metadata)&#xa;&#xa;Technology Stack:&#xa;• FastAPI&#xa;• Scikit-learn, Surprise, TensorFlow&#xa;• Pandas, NumPy&#xa;• APScheduler&#xa;• Motor (MongoDB driver)" style="text;strokeColor=none;fillColor=none;align=left;verticalAlign=top;spacingLeft=4;spacingRight=4;overflow=hidden;rotatable=0;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;" vertex="1" parent="ai-server">
          <mxGeometry y="26" width="500" height="454" as="geometry"/>
        </mxCell>

        <!-- PostgreSQL Database -->
        <mxCell id="postgres-db" value="PostgreSQL Database" style="swimlane;fontStyle=1;align=center;verticalAlign=top;childLayout=stackLayout;horizontal=1;startSize=26;horizontalStack=0;resizeParent=1;resizeParentMax=0;resizeLast=0;collapsible=1;marginBottom=0;fillColor=#f8cecc;strokeColor=#b85450;" vertex="1" parent="1">
